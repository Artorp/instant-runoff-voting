<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="5.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111">
   <children>
      <Label prefHeight="40.0" prefWidth="200.0" text="Write the name of each candidate on a new line" wrapText="true" />
      <TextArea fx:id="textareaCandidates" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="buttonAssignNew" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Assign new candidates" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
